	<key>IDECodeSnippetSummary</key>
	<string>Disables 3rd-party keyboard extensions (add to Application Delegate)</string>
	<key>IDECodeSnippetTitle</key>
	<string>Objective-C Keyboard Extensions</string>
	<key>IDECodeSnippetUserSnippet</key>
