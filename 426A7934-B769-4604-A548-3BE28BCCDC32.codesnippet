	<key>IDECodeSnippetContents</key>
	<string>- (BOOL)application:(UIApplication *)application shouldAllowExtensionPointIdentifier:(UIApplicationExtensionPointIdentifier)extensionPointIdentifier {
    
    // Prevent usage of third party keyboard extensions
    if (extensionPointIdentifier == UIApplicationKeyboardExtensionPointIdentifier) {
        return NO;
    }
    
    return YES;
    
}</string>
